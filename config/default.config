server   { # php/fastcgi
  listen       127.0.0.3:9999;
  server_name  www.listen.com listen;
  root   /var/www/html;
  index  index.php;
  cgi off;
  location /a {
    error_page 401 /error_page401;
    root /html/dfssd;
    methods GET;
    location /a/b/ {
      root /b;
    }
    location /c {
      methods POST;
      error_page 403 /sdafsd3;
      cgi off;
    }
  }
  location /d {
    root /d;
  }
  error_page 401 /sdafsd;
  return 55;
}

