server   { # php/fastcgi
  listen       localhost:9992;
  server_name  www.listen.com listen;
  root   /media/fdiraa/raven/Random/Webserv_2.0/WebServer/nginx-html;
  index  index1.html;
  autoindex on;
  cgi off;
  location /pages {
    error_page 401 /error_page401;
    root /media/fdiraa/raven/Random/Webserv_2.0/WebServer/nginx-html;
    methods GET;
    autoindex on;
    return 400 www.example.com;
    
  error_page 401 /sdafsd;
  #return 301 www.example.com;
  location *.cgi {
    root /var/www/html;
    methods GET;
    cgi_path /usr/bin/php-cgi;
    #return 301 https://www.google.com/;
  }
}
}
