server   { # php/fastcgi
  listen       90;
  server_name  www.listen.com listen;
  root   /var/www/html;
  index  index.php;
  error_page   500;
  cgi on;
  location a {
    root a;
    methods GET;
    location b {
      root b;
    }
    location c { 
      methods POST;
      cgi off;
    }
  }
  location d {
    root d;
  }
  return 55;
}

server {
  listen 1.1.1.1:400;
  root server1;
}
